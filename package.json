{
  "name": "aws-step-functions-constructor",
  "displayName": "AWS step functions constructor",
  "description": "Visualization of AWS step functions",
  "icon": "images/icon.png",
  "version": "0.8.1",
  "publisher": "paulshestakov",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulShestakov/aws-step-functions-constructor.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:extension.showStepFunction"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.showStepFunction",
        "title": "Show step function"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "dagre": "0.8.5",
    "dagre-d3": "0.6.4",
    "js-yaml": "4.1.0",
    "json-source-map": "0.6.1",
    "lodash": "4.17.21",
    "ramda": "0.27.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/dagre-d3": "0.6.2",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "4.0.1",
    "@types/lodash": "4.14.170",
    "@types/node": "15.12.4",
    "@types/ramda": "0.27.41",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "25.3.4",
    "eslint-plugin-node": "11.1.0",
    "husky": "4.3.8",
    "jest": "^27.5.1",
    "prettier": "2.3.1",
    "pretty-quick": "3.1.1",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.3",
    "typescript": "4.3.4",
    "vscode": "1.1.37",
    "webpack": "5.76.0",
    "webpack-cli": "4.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
