{
  "name": "aws-step-functions-constructor",
  "displayName": "AWS step functions constructor",
  "description": "Visualization of AWS step functions",
  "icon": "images/newPic.png",
  "version": "0.8.1",
  "publisher": "paulshestakov",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulShestakov/aws-step-functions-constructor.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:extension.showStepFunction"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.showStepFunction",
        "title": "Show step function"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "dagre": "0.8.5",
    "dagre-d3": "0.6.4",
    "js-yaml": "4.0.0",
    "json-source-map": "0.6.1",
    "lodash": "4.17.21",
    "ramda": "0.27.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/dagre-d3": "0.6.2",
    "@types/js-yaml": "4.0.0",
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.35",
    "@types/ramda": "0.27.39",
    "@types/uuid": "8.3.0",
    "prettier": "2.2.1",
    "ts-loader": "8.0.18",
    "tslint": "6.1.3",
    "typescript": "4.2.3",
    "vscode": "1.1.37",
    "webpack": "5.27.0",
    "webpack-cli": "4.5.0"
  }
}
